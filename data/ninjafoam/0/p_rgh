FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    east_face
    {
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }

    north_face
    {
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }

    south_face
    {
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }
    west_face
    {
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }
    minZ
    {
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }
    maxZ
    {
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }
}

